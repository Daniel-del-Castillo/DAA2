        READ 1
        LOAD 1
        JGTZ else
        WRITE =0
        JUMP fin
else:   LOAD 1
        STORE 2
while:  LOAD 1
        SUB = 1
        JZERO end_whi
        LOAD 2
        MULT 2
        STORE 2
        LOAD 1
        DIV =2
        STORE 1
        JUMP while
end_whi:WRITE 2
fin:    HALT